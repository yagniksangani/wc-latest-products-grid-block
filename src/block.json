{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/wc-latest-products-grid-block",
	"version": "0.1.0",
	"title": "WC Latest Products Grid Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Display the latest woocommerce products with this custom gutenberg block.",
	"supports": {
		"html": false
	},
	"attributes": {
		"gridGap": {
			"type": "integer",
			"default": 10
		},
		"displaySaleTag": {
			"type": "boolean",
			"default": 1
		},
		"displayProductTitle": {
			"type": "boolean",
			"default": 1
		},
		"displayProductPrice": {
			"type": "boolean",
			"default": 1
		},
		"displayAddToCartBtn": {
			"type": "boolean",
			"default": 1
		},
		"productTitleColor": {
			"type": "string",
			"default": "#000"
		},
		"productPriceColor": {
			"type": "string",
			"default": "#000"
		},
		"productBtnTextColor": {
			"type": "string",
			"default": "#000"
		},
		"productBtnBgColor": {
			"type": "string",
			"default": "#ccc"
		}
	},
	"textdomain": "wclpg-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}